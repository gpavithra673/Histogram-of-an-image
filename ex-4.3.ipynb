    "import cv2\n",
    "import matplotlib.pyplot as plt\n",
    "gray_image = cv2.imread(\"gray1.jpg\",0)\n",
    "cv2.imshow(\"Gray Image\",gray_image)\n",
    "equ = cv2.equalizeHist(gray_image)\n",
    "cv2.imshow(\"Equalized Image\",equ)\n",
    "cv2.waitKey(0)\n",
    "cv2.destroyAllWindows"
   ]
